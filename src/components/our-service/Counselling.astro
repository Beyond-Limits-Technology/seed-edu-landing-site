  <div class="pt-8">
    <!-- Component -->
    <!--  py-32 md:py-24 lg:py-32 -->
    <div class="relative">
      <ServiceCard
        id={1}
        stepName="Expert Guidance"
        image="https://i.postimg.cc/rFdKs14Y/1000-F-603239501-8-Hb-TZsq-ORNHxgoh-Tg-LKFs-Wo-Dfs-VE0h-AR.jpg"
        header="Expert Guidance Sessions"
        description="Unlock insights into courses, colleges, budgeting, and ideal study locations."
        features={[
          "Personalized Sessions: Receive one-on-one guidance based on your unique aspirations.",
          "Flexible Communication: Choose between online consultations or visit our Melbourne office for prompt responses.",
        ]}
      />

      <ServiceCard
        id={2}
        stepName="In-Depth Exploration"
        image="https://i.postimg.cc/kMspG8C8/GT-story.jpg"
        header="In-Depth Exploration"
        description="Unlock detailed insights into potential programs, institutions, and budget considerations."
        features={[
          "Comprehensive Guidance: Our counsellors delve into the specifics of your academic goals.",
          "Clear Communication: Understand the nuances of your academic journey through transparent discussions.",
        ]}
      />

      <ServiceCard
        id={3}
        stepName="Decision Support"
        image="https://i.postimg.cc/cLkKLF9j/126039981-gettyimages-114727959.jpg"
        header="Decision Support"
        description="Empower yourself with the right information to make informed decisions for your education."
        features={[
          "Doubt Clearance: Dedicated counsellors resolve any uncertainties you may have.",
          "Education Journey Planning: Collaborate with us to chart the path to your academic success.",
        ]}
      />
    </div>
  </div>
