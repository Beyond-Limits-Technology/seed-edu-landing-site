---
import Layout from "../../../layouts/Layout.astro";
import Breadcrumb from "../../../components/common/Breadcrumb.astro";
import Content from "../../../components/student-essentials/oversea-student-health-cover/Content.astro";
---

<Layout title="SEED | Student Accomondation">
  <div class="relative h-[350px] overflow-hidden">
    <div
      class="absolute inset-0 bg-cover bg-[url('https://images.unsplash.com/photo-1504813184591-01572f98c85f?q=80&w=1471&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D')] bg-center bg- w-screen min-h-[350px] bg-no-repeat brightness-75"
    >
    </div>
    <div class="absolute inset-0 bg-black opacity-50"></div>
    <div
      class="absolute inset-0 flex flex-col justify-center items-center text-white"
    >
      <h1 class="sm:text-7xl text-center text-4xl font-bold">
        Oversea Student Health Cover
      </h1>
      <Breadcrumb
        routeName="Over Sea Student Health Cover"
        link="essentials/student-accommodation"
      />
      <p class="sm:text-lg text-base mt-6 text-center">
        Your Well-Being, Our Priority
      </p>
    </div>
  </div>
  <Content />
</Layout>
